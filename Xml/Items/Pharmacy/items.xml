  <Items>
    <!-- Courtesy of Grass's Pill bottles mod -->
    <Item name="" identifier="custompill" category="Medical" cargocontaineridentifier="mediccrate" Tags="smallitem,chem,medical,pill" description="" useinhealthinterface="true" scale="0.1" impactsoundtag="impact_pill" maxstacksize="16">
      <PreferredContainer primary="medcab" spawnprobability="0" />
      <PreferredContainer primary="wreckmedcab,abandonedmedcab" minamount="0" maxamount="8" spawnprobability="0.9" />
      <PreferredContainer primary="outposttrashcan" minamount="0" maxamount="2" spawnprobability="0.1" />
      <Price baseprice="50" soldbydefault="false">
        <Price storeidentifier="merchantmedical" sold="true" multiplier="0.9" minavailable="16" />
      </Price>
      <Fabricate suitablefabricators="vendingmachine" requiredtime="1" requiredmoney="50" fabricationlimitmin="5" fabricationlimitmax="10" />
      <Deconstruct time="5" />
      <InventoryIcon texture="%ModDir%/Images/Pharmacy/InventoryItemIconAtlas.png" sourcerect="0,0,64,64" origin="0.5,0.5" />
      <Sprite texture="%ModDir%/Images/Pharmacy/droppedpills.png" sourcerect="103,0,113,31" depth="0.6" origin="0.5,0.5" />
      <Body width="50" height="30" density="8" />
      <Holdable canBeCombined="true" removeOnCombined="true" slots="Any,RightHand,LeftHand" handle1="0,0" msg="ItemMsgPickUpSelect">
        <!-- Remove the item when fully used -->
        <StatusEffect type="OnBroken" target="This">
          <Remove />
        </StatusEffect>
        <!-- randomize effects if spawned outside of the chem master -->
        <StatusEffect type="OnSpawn" target="This" delay="0.5" stackable="false">
          <LuaHook name="NTL.OnPillSpawned" />
        </StatusEffect>
      </Holdable>
    </Item>
    <Item name="" identifier="custompill_horsepill" variantof="custompill" scale="0.09" impactsoundtag="impact_horsepill" maxstacksize="8">
      <InventoryIcon texture="%ModDir%/Images/Pharmacy/InventoryItemIconAtlas.png" sourcerect="64,0,64,64" origin="0.5,0.5" />
